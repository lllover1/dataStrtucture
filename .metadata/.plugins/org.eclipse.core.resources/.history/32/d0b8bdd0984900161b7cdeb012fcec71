package adt;

public class Node<T> {
	
	//=====================================================================
	//类私有属性
	private T data;
	private Node<T> next;
	
	//=====================================================================
	//构造方法
	public Node (){
		this(null,null);
	}
	
	public Node (T data) {
		this(data,null);
	}
	
	public Node (T data,Node<T> next) {
		this.data = data;
		this.next = next;
	}
}
