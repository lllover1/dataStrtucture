package adt;

public class Node<T> {
	
	//=====================================================================
	//类私有属性
	private T data;                 //节点存入的值
	private Node<T> next;           //下一个节点
	
	//=====================================================================
	//构造方法
	public Node (){
		this(null,null);
	}
	
	public Node (T data) {
		this(data,null);
	}
	
	public Node (T data,Node<T> next) {
		this.data = data;
		this.next = next;
	}
	//======================================================================
	//get()方法，set()方法
}
